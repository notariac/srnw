\par \pard\plain \s0\ql\nowidctlpar\ltrpar{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\cf0\kerning1\dbch\af23\langfe2052\dbch\af24\afs24\alang1081\loch\f5\fs24\lang3082{\listtext\pard\plain  a)\tab}\ilvl0\ls8 \li1442\ri0\lin1442\rin0\fi-360\qj\tqr\tx12962\li722\ri0\lin722\rin0\fi-519{\strike0\ulnone\ulc1\dbch\af26\afs28\rtlch \ltrch\loch\fs28\loch\f6
